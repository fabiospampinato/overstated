{
  "name": "overstated",
  "description": "A wrapper around unstated, with many useful plugins built-in, for easier development.",
  "version": "1.0.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc --skipLibCheck && tstei",
    "compile:watch": "tsc --skipLibCheck --watch",
    "prepublishOnly": "npm run clean && npm run compile"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/overstated/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/overstated.git"
  },
  "keywords": [
    "unstated",
    "plugins"
  ],
  "dependencies": {
    "unstated": "^2.1.1",
    "unstated-compose": "^1.0.5",
    "unstated-connect2": "^1.1.7",
    "unstated-debug": "^0.3.2",
    "unstated-hmr": "^1.0.6",
    "unstated-suspense-autosuspend": "^1.0.1",
    "unstated-suspense-middleware": "^1.0.3"
  },
  "devDependencies": {
    "@types/node": "^10.9.4",
    "rimraf": "^2.6.0",
    "typescript": "^3.2.2",
    "typescript-transform-export-interop": "^1.0.0"
  }
}
